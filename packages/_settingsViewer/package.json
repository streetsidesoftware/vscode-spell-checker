{
  "name": "settings-webview",
  "private": true,
  "version": "2.0.0-alpha.1",
  "description": "Webview for VS Code to edit cspell settings",
  "typings": "out",
  "main": "out",
  "scripts": {
    "build": "tsc -p . && yarn run webpack",
    "clean": "rimraf ../client/settingsViewer ./out",
    "build-production": "yarn run clean && yarn run build",
    "coverage": "jest --coverage src",
    "lint": "eslint 'src/**/*.ts'",
    "start:dev": "webpack-dev-server --config webpack.dev.js",
    "webpack": "webpack --config webpack.prod.js",
    "test": "jest src"
  },
  "keywords": [],
  "author": "Jason Dent",
  "license": "MIT",
  "devDependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@types/clone-deep": "^4.0.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-test-renderer": "^17.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "browser-resolve": "^2.0.0",
    "clone-deep": "^4.0.1",
    "eslint-plugin-react": "^7.23.2",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "mobx": "^6.3.0",
    "mobx-react": "^7.1.0",
    "mobx-react-devtools": "^6.1.1",
    "mobx-react-lite": "^3.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "resolve-url-loader": "^4.0.0",
    "terser": "^5.7.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "vscode-uri": "^3.0.2",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "files": [
    "out/api/**",
    "out/webapp/**",
    "!**/test*",
    "!**/*.map",
    "!**/*.test.*",
    "!**/*.spec.*"
  ]
}
