{
  "name": "settings-webview",
  "private": true,
  "version": "2.0.0",
  "description": "Webview for VS Code to edit cspell settings",
  "typings": "out",
  "main": "out",
  "scripts": {
    "build": "tsc -p . && yarn run webpack",
    "clean": "shx rm -rf ../client/settingsViewer ./out",
    "build-production": "yarn run clean && yarn run build",
    "coverage": "jest --coverage src",
    "start:dev": "webpack server --config webpack.dev.js",
    "webpack": "webpack --config webpack.prod.js",
    "watch": "yarn webpack --watch",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "Jason Dent",
  "license": "MIT",
  "devDependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@types/clone-deep": "^4.0.1",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.19",
    "@types/react-test-renderer": "^17.0.2",
    "browser-resolve": "^2.0.0",
    "clone-deep": "^4.0.1",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "^5.5.0",
    "locale-resolver": "*",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "mobx-react-devtools": "^6.1.1",
    "mobx-react-lite": "^3.4.3",
    "path-browserify": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "terser": "^5.16.8",
    "terser-webpack-plugin": "^5.3.7",
    "ts-loader": "^9.4.2",
    "vscode-uri": "^3.0.7",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1"
  },
  "files": [
    "out/api/**",
    "out/webapp/**",
    "!**/test*",
    "!**/*.map",
    "!**/*.test.*",
    "!**/*.spec.*"
  ],
  "engines": {
    "node": ">16.0.0"
  }
}
