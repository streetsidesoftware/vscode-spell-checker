{
  "name": "@internal/settings-webview",
  "private": true,
  "version": "2.0.0",
  "description": "Webview for VS Code to edit cspell settings",
  "typings": "out",
  "main": "out",
  "scripts": {
    "build": "tsc -p . && npm run webpack",
    "clean": "shx rm -rf ../client/settingsViewer ./out",
    "build-production": "npm run clean && npm run build",
    "coverage": "jest --coverage src",
    "start:dev": "webpack server --config webpack.dev.js",
    "webpack": "webpack --config webpack.prod.js",
    "watch": "npm webpack --watch",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "Jason Dent",
  "license": "MIT",
  "devDependencies": {
    "@internal/locale-resolver": "file:../__locale-resolver",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@types/clone-deep": "^4.0.2",
    "@types/react-dom": "^17.0.21",
    "@types/react-test-renderer": "^17.0.5",
    "@types/react": "^17.0.67",
    "browser-resolve": "^2.0.0",
    "clone-deep": "^4.0.1",
    "eslint-plugin-react": "^7.33.2",
    "html-webpack-plugin": "^5.5.3",
    "mobx-react-lite": "^3.4.3",
    "mobx-react": "^7.6.0",
    "mobx": "^6.10.2",
    "path-browserify": "^1.0.1",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "react": "^17.0.2",
    "terser-webpack-plugin": "^5.3.9",
    "terser": "^5.21.0",
    "ts-loader": "^9.4.4",
    "vscode-uri": "^3.0.7",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack": "^5.88.2"
  },
  "files": [
    "out/api/**",
    "out/webapp/**",
    "!**/test*",
    "!**/*.map",
    "!**/*.test.*",
    "!**/*.spec.*"
  ],
  "engines": {
    "node": ">18.0.0"
  }
}
