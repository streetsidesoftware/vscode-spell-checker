{
  "name": "settings-webview",
  "private": true,
  "version": "2.0.0-alpha.1",
  "description": "Webview for VS Code to edit cspell settings",
  "typings": "out",
  "main": "out",
  "scripts": {
    "build": "tsc -p . && yarn run webpack",
    "clean": "rimraf ../client/settingsViewer ./out",
    "build-production": "yarn run clean && yarn run build",
    "coverage": "jest --coverage src",
    "lint": "eslint 'src/**/*.ts'",
    "start:dev": "webpack server --config webpack.dev.js",
    "webpack": "webpack --config webpack.prod.js",
    "watch": "yarn webpack --watch",
    "test": "jest src"
  },
  "keywords": [],
  "author": "Jason Dent",
  "license": "MIT",
  "devDependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@types/clone-deep": "^4.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "browser-resolve": "^2.0.0",
    "clone-deep": "^4.0.1",
    "eslint-plugin-react": "^7.27.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "mobx": "^6.3.7",
    "mobx-react": "^7.2.1",
    "mobx-react-devtools": "^6.1.1",
    "mobx-react-lite": "^3.2.2",
    "path-browserify": "^1.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "resolve-url-loader": "^4.0.0",
    "terser": "^5.9.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "url-loader": "^4.1.1",
    "vscode-uri": "^3.0.2",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "files": [
    "out/api/**",
    "out/webapp/**",
    "!**/test*",
    "!**/*.map",
    "!**/*.test.*",
    "!**/*.spec.*"
  ]
}
