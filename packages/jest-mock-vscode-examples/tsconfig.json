{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Jest Mock for VS Code Typescript Config",
	"extends": "@tsconfig/node14/tsconfig.json",
	"compilerOptions": {
		"declaration": true,
		"declarationMap": true,
		"exactOptionalPropertyTypes": true,
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"sourceMap": true,
		"outDir": "dist",
		"rootDir": "src"
	},
	"include": [
		"src"
	],
	"exclude": []
}
