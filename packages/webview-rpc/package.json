{
  "name": "vscode-webview-rpc",
  "private": true,
  "version": "1.0.0",
  "description": "Make JSON-RPC calls to VSCode Webviews",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run build:tsc",
    "build:tsc": "tsc -p tsconfig.json",
    "build-production": "npm run clean && npm run build",
    "clean": "shx rm -rf dist",
    "coverage": "echo ok",
    "test": "echo ok"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js"
    },
    "./webview": {
      "import": "./dist/esm/webview/index.js"
    },
    "./extension": {
      "import": "./dist/esm/extension/index.js"
    }
  },
  "keywords": [
    "json-rpc",
    "webviews",
    "vscode"
  ],
  "author": "Street Side Software",
  "license": "MIT",
  "dependencies": {
    "json-rpc-api": "file:../json-rpc-api",
    "utils-disposables": "file:../utils-disposables",
    "vscode-jsonrpc": "^8.2.1"
  },
  "files": [
    "dist",
    "!dist/**/*.map",
    "!dist/**/*.test.*",
    "!dist/**/test/**",
    "!dist/esm/**/*.js",
    "!dist/esm/**/*.ts"
  ],
  "engines": {
    "node": ">=22.20.0"
  },
  "devDependencies": {}
}
