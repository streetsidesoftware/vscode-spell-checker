{
  "name": "settings-viewer-next",
  "description": "Settings Viewer for Code Spell Checker",
  "version": "0.0.1",
  "publisher": "streetsidesoftware",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/streetsidesoftware/vscode-spell-checker"
  },
  "engines": {
    "vscode": "^1.74.0",
    "node": ">16.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onWebviewPanel:catCoding"
  ],
  "main": "./out/extension.js",
  "types": "./out/extension.d.ts",
  "contributes": {
    "commands": [
      {
        "command": "catCoding.start",
        "title": "Start cat coding session",
        "category": "Cat Coding"
      },
      {
        "command": "catCoding.doRefactor",
        "title": "Do some refactoring",
        "category": "Cat Coding"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "clean": "shx rm -rf out dist coverage temp",
    "clean-build": "yarn run clean && yarn run build",
    "build": "yarn compile",
    "build-production": "yarn run clean-build",
    "compile": "tsc -p ./",
    "test": "echo test",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.18.16",
    "@types/vscode": "^1.76.0",
    "@types/vscode-webview": "^1.57.1",
    "typescript": "^4.9.5"
  }
}
