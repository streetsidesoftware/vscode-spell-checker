{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Client: Launch Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--disable-extensions",
                "--extensionDevelopmentPath=${workspaceRoot}"
            ],
            "sourceMaps": true,
            "outFiles": [
                "${workspaceRoot}/packages/client/dist/**"
            ],
            "smartStep": true,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
