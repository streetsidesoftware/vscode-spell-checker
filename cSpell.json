        "hte"
    ],
    "ignoreWords": [],
    "dictionaryDefinitions": [
        {
            "name": "cities",
            "path": "./sampleDictionaries/cities.txt"
        }
    ],
    "dictionaries": [
        "cities"
    ],
    "languageSettings": [
        {
            "languageId": "*",
            "dictionaries": []
        }
    ]
}