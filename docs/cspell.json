{
    "import": [
        "../cSpell.json"
    ],
    "ignorePaths": [
        "sitemap.xml",
        "_sass/minimal-mistakes/**",
        "_sass/minimal-mistakes.*",
        "_site",
        "_site/**"
    ],
    "dictionaries": [
        "custom-dictionary"
    ],
    "dictionaryDefinitions": [
        {
            "name": "custom-dictionary",
            "path": "./.cspell/custom-dictionary.txt",
            "addWords": true
        }
    ],
    "enableFiletypes": [
        "ruby"
    ],
    "overrides": [
        {
            "filename": "Gemfile",
            "languageId": "ruby"
        }
    ]
}
